{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": ["${mod_authors}"],
  "contact": {
    "sources": "${mod_sources}",
    "issues": "${mod_issue_tracker}"
  },
  "license": "${mod_license}",
  "icon": "assets/botumi/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.behindthescenery.botumi.fabric.BotumiFabric"
    ],
    "client": [
      "dev.behindthescenery.botumi.fabric.client.BotumiFabricClient"
    ]
  },
  "mixins": [
    "botumi.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": ">=${minecraft_version}",
    "java": ">=21",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}
